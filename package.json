{
  "name": "webpack-typescript-tailwind",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "webpack": "npx webpack ",
    "typescript": "tsc",
    "format": "npx prettier --write .",
    "test": "jest",
    "dev": "webpack server",
    "start": "webpack-dev-server --open --mode development --hot",
    "build": "webpack --mode production"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/__tests__/stubs/styles.js",
      "\\.(png|jpg|jpeg)$": "<rootDir>/__tests__/stubs/images.js"
    },
    "testMatch": [
      "**/?(*.)+(test).[jt]s?(x)"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-transform-typescript": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^28.1.4",
    "@types/react-dom": "^18.0.6",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.2",
    "prettier": "2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@material-ui/core": "^4.12.4",
    "@mui/material": "^5.10.8",
    "@mui/system": "^5.10.8",
    "@reduxjs/toolkit": "^1.8.6",
    "@types/material-ui": "^0.21.12",
    "@types/react": "^18.0.15",
    "jest-cli": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "lodash": "^4.17.21",
    "material-ui-colors": "^1.0.0",
    "react-redux": "^8.0.4",
    "require": "^2.4.20",
    "ts-jest": "^28.0.5"
  }
}
